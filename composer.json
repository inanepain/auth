{
    "name": "inanepain/auth",
    "description": "Authentication adapters for common use cases.",
    "type": "library",
    "license": "Unlicense",
    "homepage": "https://github.com/inanepain/auth",
    "readme": "README.md",
    "authors": [
        {
            "name": "Philip Michael Raab",
            "email": "philip@cathedral.co.za",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "philip@cathedral.co.za",
